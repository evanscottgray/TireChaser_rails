<%= form_for @vendor.tires.new do |tire| %>
    <%= tire.label :name %>
    <br />
    <%= tire.text_field :name %>
    <br />
    <%= tire.label :brand%>
    <br />
    <%= tire.text_field :brand %>
    <br />
    <%= tire.label :price%>
    <br />
    <%= tire.number_field :price %>
    <br />
    <%= tire.label :quantity%>
    <br />
    <%= tire.number_field :quantity %>
    <br />
    <%= tire.submit %>
<% end %>
